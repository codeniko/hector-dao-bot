    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.9",
    "typedoc-plugin-no-inherit": "^1.3.1",
    "typescript": "^4.5.2"
